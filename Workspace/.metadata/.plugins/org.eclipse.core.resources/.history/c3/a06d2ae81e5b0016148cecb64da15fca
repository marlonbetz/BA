import matplotlib.pyplot as plt
from pandas import DataFrame
import codecs
import pickle
import numpy as np
from sklearn.manifold import TSNE
langs = pickle.load(codecs.open("/Users/marlon/Documents/BA/Workspace/Workspace/embeddings/local_manifold_learning/langs.pkl","rb"))

langs_np = []
lang_names= []
for lang in langs:
    lang_names.append(lang)
    langs_np.append(langs[lang])

langs_np = np.array(langs_np)

tsne = TSNE(2)
langs_np_tsne = tsne.fit_transform(langs_np)