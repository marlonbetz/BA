from pipeline import *

from scipy.stats import spearmanr,pearsonr

cluster1 = np.random.normal(0,1,(10,2))
cluster2 = np.random.normal(1,.7,(15,2))
cluster3 = np.random.normal(-1,1.3,(7,2))
cluster4 = np.random.normal(0,10,(20,2))
X = np.array([cluster1,cluster2,cluster3,cluster4]).reshape(-1,2)
import matplotlib.pyplot as plt
plt.scatter(X[:,0],X[:,1])
plt.show()