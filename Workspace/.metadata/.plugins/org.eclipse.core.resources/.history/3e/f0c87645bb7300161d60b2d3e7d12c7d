from pipeline import *

from scipy.stats import spearmanr,pearsonr

cluster1 = np.random.normal(0,1,(20,2))
cluster2 = np.random.normal(1,.7,(15,2))
cluster3 = np.random.normal(-1,1.3,(14,2))
cluster4 = np.random.normal(0,3,(40,2))
X = np.append(cluster1,np.append(cluster2,np.append(cluster3,cluster4,axis=0),axis=0),axis=0)
import matplotlib.pyplot as plt
plt.scatter(X[:,0],X[:,1])
plt.show()